{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": ["px", "em", "rem", "vh", "vw", "%"],
			"padding": true,
			"margin": true,
			"blockGap": true
		},
		"color": {
			"custom": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#21759b",
					"name": "Primary"
				},
				{
					"slug": "foreground",
					"color": "#333333",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#f3f1ee",
					"name": "Background"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "off-white",
					"color": "#fcfcfc",
					"name": "Off White"
				},
				{
					"slug": "light-gray",
					"color": "#eeeeee",
					"name": "Light Gray"
				},
				{
					"slug": "gray",
					"color": "#888888",
					"name": "Gray"
				},
				{
					"slug": "muted",
					"color": "#666666",
					"name": "Muted"
				},
				{
					"slug": "emphasis",
					"color": "#000000",
					"name": "Emphasis"
				},
				{
					"slug": "success",
					"color": "#32d296",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#faa05a",
					"name": "Warning"
				},
				{
					"slug": "danger",
					"color": "#f0506e",
					"name": "Danger"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"fontSizes": [
				{
					"slug": "x-small",
					"size": "0.6875rem",
					"name": "X-Small"
				},
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "article-small",
					"size": "1.1rem",
					"name": "Article Small"
				},
				{
					"slug": "article",
					"size": "1.4rem",
					"name": "Article"
				},
				{
					"slug": "large",
					"size": "1.75rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "2.25rem",
					"name": "X-Large"
				}
			],
			"fontFamilies": [
				{
					"slug": "sans-serif",
					"fontFamily": "\"游ゴシック体\", YuGothic, \"游ゴシック Medium\", \"Yu Gothic Medium\", \"游ゴシック\", \"Yu Gothic\", sans-serif",
					"name": "Sans Serif"
				},
				{
					"slug": "serif",
					"fontFamily": "\"游明朝体\", \"Yu Mincho\", YuMincho, serif",
					"name": "Serif"
				},
				{
					"slug": "monospace",
					"fontFamily": "consolas, lucida, Osaka, monospace",
					"name": "Monospace"
				}
			]
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/group": {
				"spacing": {
					"padding": true,
					"margin": true,
					"blockGap": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "500",
			"lineHeight": "1.7"
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":visited": {
					"color": {
						"text": "#747fff"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.5rem",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.25rem",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			}
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.8"
				}
			}
		}
	},
	"customTemplates": [],
	"templateParts": []
}
