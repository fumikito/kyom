{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "kyom/testimonials",
	"version": "1.0.0",
	"title": "Testimonials",
	"category": "widgets",
	"icon": "star-filled",
	"description": "お客様の声（jetpack-testimonial）をスライダーで表示します。",
	"keywords": ["testimonials", "お客様の声", "reviews"],
	"textdomain": "kyom",
	"attributes": {
		"number": {
			"type": "number",
			"default": -1
		},
		"order": {
			"type": "string",
			"default": "random",
			"enum": ["random", "desc", "post_order"]
		}
	},
	"supports": {
		"html": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}
